{
  "author": "Adam Meester",
  "name": "@adammeester/component-library",
  "description": "Adam Meester's component library",
  "version": "0.4.5",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "storybook dev -p 6006",
    "build": "rollup -c",
    "build:nextjs": "MODULE=esm PUBLISH_CONFIG=nextjs rollup -c",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/adammeester/component-library.git"
  },
  "publishConfig": {
    "@adammeester:registry": "https://npm.pkg.github.com"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "dist",
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react-dom": "^18.0.11",
    "@vanilla-extract/css": "^1.11.0",
    "@vanilla-extract/recipes": "^0.3.0",
    "eslint": "8.34.0",
    "react": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-select": "^5.7.0",
    "react-spinners": "^0.13.8",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-interactions": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/blocks": "^7.0.5",
    "@storybook/react": "^7.0.5",
    "@storybook/react-webpack5": "^7.0.5",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.0.38",
    "@vanilla-extract/rollup-plugin": "^1.2.1",
    "@vanilla-extract/webpack-plugin": "^2.2.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "mini-css-extract-plugin": "^2.7.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.21.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-node-externals": "^5.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sb": "^7.0.5",
    "storybook": "^7.0.5",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0"
  }
}
